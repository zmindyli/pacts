{
    "compilerOptions": {
      "target": "es6",
      "outDir": "./output",
      "lib": ["dom", "dom.iterable", "esnext"],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "importsNotUsedAsValues": "error",
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true
    },
    "types": ["node"],
    "include": ["./*"],
    "exclude": []
  }